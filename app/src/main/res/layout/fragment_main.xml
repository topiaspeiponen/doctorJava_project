<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="192dp" />

    <ImageButton
        android:id="@+id/CoinBuilding"
        android:layout_width="166dp"
        android:layout_height="153dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/UpgradeBuilding1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@color/colorAccent" />

    <ImageButton
        android:id="@+id/UpgradeBuilding1"
        android:layout_width="166dp"
        android:layout_height="153dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/holo_blue_dark" />

    <ImageButton
        android:id="@+id/UpgradeBuilding2"
        android:layout_width="166dp"
        android:layout_height="153dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/UpgradeBuilding4"
        app:layout_constraintEnd_toStartOf="@+id/UpgradeBuilding3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CoinBuilding"
        app:srcCompat="@android:color/holo_green_light" />

    <ImageButton
        android:id="@+id/UpgradeBuilding3"
        android:layout_width="166dp"
        android:layout_height="153dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/UpgradeBuilding5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/UpgradeBuilding1"
        app:srcCompat="@android:color/holo_purple" />

    <ImageButton
        android:id="@+id/UpgradeBuilding4"
        android:layout_width="166dp"
        android:layout_height="153dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/UpgradeBuilding5"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:color/holo_orange_dark" />

    <ImageButton
        android:id="@+id/UpgradeBuilding5"
        android:layout_width="166dp"
        android:layout_height="153dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:color/darker_gray" />

    <ImageView
        android:id="@+id/Arrow1"
        android:layout_width="33dp"
        android:layout_height="64dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/UpgradeBuilding1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/CoinBuilding"
        app:layout_constraintTop_toTopOf="@+id/fragment_container"
        app:srcCompat="@drawable/arrowleft" />

    <ImageView
        android:id="@+id/Arrow2"
        android:layout_width="33dp"
        android:layout_height="64dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/UpgradeBuilding3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/UpgradeBuilding2"
        app:layout_constraintTop_toBottomOf="@+id/Arrow1"
        app:srcCompat="@drawable/arrowright" />

    <ImageView
        android:id="@+id/Arrow3"
        android:layout_width="33dp"
        android:layout_height="64dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="60dp"
        app:layout_constraintBottom_toBottomOf="@+id/fragment_container"
        app:layout_constraintStart_toEndOf="@+id/UpgradeBuilding4"
        app:layout_constraintTop_toBottomOf="@+id/Arrow2"
        app:layout_constraintVertical_bias="0.946"
        app:srcCompat="@drawable/arrowleft" />

    <ImageView
        android:id="@+id/Arrow4"
        android:layout_width="27dp"
        android:layout_height="56dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="156dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.225"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/arrowup" />

    <ImageView
        android:id="@+id/Arrow5"
        android:layout_width="27dp"
        android:layout_height="56dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.763"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.383"
        app:srcCompat="@drawable/arrowup" />


</androidx.constraintlayout.widget.ConstraintLayout>
